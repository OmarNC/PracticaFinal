    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/animals_nav"
    app:startDestination="@id/animalListFragment">

    <fragment
        android:id="@+id/animalListFragment"
        android:name="com.onc.practicafinal.view.fragments.AnimalListFragment"
        android:label="fragment_animal_list"
        tools:layout="@layout/fragment_animal_list" >
        <action
            android:id="@+id/action_animalListFragment_to_animalDetailFragment2"
            app:destination="@id/animalDetailFragment2" />
    </fragment>
    <fragment
        android:id="@+id/animalDetailFragment2"
        android:name="com.onc.practicafinal.view.fragments.AnimalDetailFragment"
        android:label="fragment_animal_detail"
        tools:layout="@layout/fragment_animal_detail" />
